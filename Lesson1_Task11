public class MyJavaApp {
      public static void main(String[] args) {
          
          int a=1;
          int b=2;
          int c=3;

          if (a >= b  && b>= c);{
          System.out.println(" The biggest: " +a);}
          else if(b >= a && a > c);{
          System.out.println(" The biggest: " +b);}
          else if (c<b);
          System.out.println(" The biggest: " +c);
      }
   }
}
