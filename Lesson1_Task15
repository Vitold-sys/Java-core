public class MyJavaApp {
      public static void main(String[] args) {
      //1 type
      for (int i=0; i<100; i++)
          System.out.println(" Number 1 to 100: " +i);
      //2 type
          int j=0;
          do {
              System.out.println("Number 1 to 100: " +j);
              j++;
          }
          while (j<100);
      //3 type
          int a = 0;
          while(a < 100) {
              System.out.println("Number 1 to 100: " +a);
              a++;
         }
