public class MyJavaApp {
      public static void main(String[] args) {
          int a=2;
          int b=5;
          int i=0;
          if (a > b);{
          i= a+3;
              System.out.println(" Case: " +i);}
           else {
          b+=8;}
          System.out.println(" Case: " +a);
          System.out.println(" Case: " +b);
     }
}
