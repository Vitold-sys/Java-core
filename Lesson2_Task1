package Myfirstapp;

import java.sql.SQLOutput;
import java.util.Arrays;
import java.util.Scanner;
import java.util.SortedMap;
import java.util.stream.IntStream;
public class MyJavaApp {
    public static void main(String[] args) {
    int[] anArray;
    anArray = new int[6];
    anArray[0] = 100;
    anArray[1] = 40;
    anArray[2] = 20;
    anArray[3] = 30;
    anArray[4] = 10;
    anArray[5] = 50;
    for (int v : anArray)
    System.out.println(v);
    Arrays.sort(anArray);
    System.out.println("Sorted numeric array : "+Arrays.toString(anArray));
    String[] anArray2 = {
                "Java",
                "Python",
                "PHP",
                "C#",
                "JavaScript",
                "C++"
            };
    Arrays.sort(anArray2);
    System.out.println("Sorted numeric array : "+Arrays.toString(anArray2));
    }
}
